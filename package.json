{
  "name": "wraith",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "expo:web": "expo start --web",
    "dev": "vite",
    "dev:web": "vite",
    "build": "vite build",
    "build:web": "vite build",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:api": "playwright test e2e/api.spec.ts",
    "test:live": "tsx e2e/live-tests/runner.ts",
    "test:live:dashboard": "tsx e2e/live-tests/runner.ts --suite dashboard",
    "test:live:trading": "tsx e2e/live-tests/runner.ts --suite trading",
    "test:live:full": "tsx e2e/live-tests/runner.ts --suite full-flow",
    "test:all": "vitest run && playwright test",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "sanity": "node scripts/ui-sanity-check.mjs",
    "eas:build:dev": "eas build --profile development --platform all",
    "eas:build:preview": "eas build --profile preview --platform all",
    "eas:build:production": "eas build --profile production --platform all",
    "eas:update:dev": "eas update --channel development --message",
    "eas:update:preview": "eas update --channel preview --message",
    "eas:update:production": "eas update --channel production --message",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "desktop:dev": "npm run tauri:dev",
    "desktop:build": "npm run tauri:build",
    "desktop:build:mac": "tauri build --target universal-apple-darwin",
    "desktop:build:win": "tauri build --target x86_64-pc-windows-msvc",
    "desktop:build:linux": "tauri build --target x86_64-unknown-linux-gnu"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.11.0",
    "babel-preset-expo": "^54.0.10",
    "expo": "~54.0.33",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-status-bar": "^3.0.9",
    "expo-updates": "~0.29.9",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "lightweight-charts": "^5.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^16.5.4",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.2",
    "react-router-dom": "^7.13.0",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.1.0",
    "@tauri-apps/api": "^2.1.0",
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.58.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "@types/react-native": "^0.72.8",
    "@types/react-native-web": "^0.19.2",
    "@types/react-window": "^1.8.8",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^27.4.0",
    "puppeteer": "^24.36.1",
    "tsx": "^4.21.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.18"
  }
}
